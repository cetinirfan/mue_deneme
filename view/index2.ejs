<%- include('./src/header'); %> <%- include('./src/headerbar'); %>
<div id="content" class="main-content">
  <div class="layout-px-spacing">
    <div class="row layout-top-spacing">
      <div class="col-xl-6 col-lg-12 col-md-12 col-sm-12 col-12 layout-spacing">
        <div class="widget widget-table-two">
          <div class="widget-heading">
            <h5 class="">Son Eklenen Tarama Testleri</h5>
          </div>

          <div class="widget-content">
            <div class="table-responsive">
              <table class="table">
                <thead>
                  <tr>
                    <th><div class="th-content">Tarama Adı</div></th>
                    <th><div class="th-content">Sınıf Düzeyi</div></th>
                    <th><div class="th-content">Soru Sayısı</div></th>
                    <th>
                      <div class="th-content th-heading">Bilgi</div>
                    </th>
                  </tr>
                </thead>
                <tbody>
                  <% find_test.forEach(function(test){ %>
                  <tr>
                    <td>
                      <div class="td-content customer-name"><%= test.name %></div>
                    </td>
                    <td>
                      <div class="td-content product-brand"><%= test.classType %></div>
                    </td>
                    <td><div class="td-content"><%= test.questionCount %></div></td>
                    <td>
                      <% if(test.information === true){ %>
                        <div class="td-content"><span class="badge outline-badge-success">Çözüldü</span></div>
                        <% } %>
                        <% if(test.information === false){ %>
                          <div class="td-content"><span class="badge outline-badge-danger">Çözülmedi</span></div>
                          <% } %>
                    </td>
                  </tr>
                  <% }); %>
                </tbody>
              </table>
            </div>
          </div>
        </div>
      </div>
    </div>
    <%- include('./src/footer'); %>
  </div>
</div>
