<%- include('./src/header'); %>
<%- include('./src/headerbar'); %>

        <div id="content" class="main-content">
            <div class="layout-px-spacing">
                
                <div class="row layout-top-spacing" id="cancel-row">
                
                    <div class="col-xl-12 col-lg-12 col-sm-12  layout-spacing">
                        <div class="widget-content widget-content-area br-6">
                            <div class="table-responsive mb-4 mt-4">
                                <table id="html5-extension" class="table table-hover non-hover" style="width:100%">
                                    <thead>
                                        <tr>
                                            <th>Test Adı</th>
                                            <th>Soru Sayısı</th>
                                            <th>Doğru Sayısı</th>
                                            <th>Yanlış Sayısı</th>
                                            <th>Boş Sayısı</th>
                                            <th>Net</th>
                                            <th>Yüzdelik Başarı</th>
                                        </tr>
                                    </thead>
                                    <tbody>
                                        <% find_sonuc.forEach(function(sonuc){ %> 
                                        <tr>
                                            <td><%= sonuc.testName %></td>
                                            <td><%= sonuc.countQuestion %></td>
                                            <td><%= sonuc.correctQuestion %></td>
                                            <td><%= sonuc.wrongQuestion %></td>
                                            <td><%= sonuc.emptyQuestion %></td>
                                            <td><%= sonuc.netQuestion %></td>
                                            <td>%<%= sonuc.percentQuestion %></td>
                                        </tr>
                                        <% }); %> 
                                    </tbody>
                                </table>
                            </div>
                        </div>
                    </div>

                </div>

                </div>
        
        <!--  END CONTENT AREA  -->
<%- include('./src/footer'); %>